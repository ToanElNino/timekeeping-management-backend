{
  "name": "marketplace",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:prod-api": "node dist/src/main",
    "start:prod-worker": "node dist/main",
    "seed:all": "cross-env ENTITY=all ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "seed:admin": "cross-env ENTITY=admin ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "seed:account": "cross-env ENTITY=account ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "seed:kms-cmk": "cross-env ENTITY=kms-cmk ts-node -r tsconfig-paths/register src/modules/seeders/seed.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm migration:run -- -d  src/config/database.config.ts",
    "migration:create": "npm run typeorm migration:create -- -n InitDatabases",
    "migration-generate": "npm run typeorm  migration:generate -- -d src/config/database.config.ts src/database/migrations/InitDatabases",
    "sync-database": "npm run migration-generate && npm run migration:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest  --runInBand --forceExit --config ./test/jest-e2e.json ",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix"
  },
  "dependencies": {
    "@ethereumjs/common": "^2.6.0",
    "@ethereumjs/tx": "^3.5.0",
    "@nestjs-modules/mailer": "^1.3.17",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "8.0.3",
    "@types/cache-manager": "^4.0.1",
    "@types/cron": "^1.7.3",
    "@types/lodash": "^4.14.168",
    "@types/passport-jwt": "^3.0.4",
    "argon2": "^0.30.2",
    "await-lock": "^2.1.0",
    "aws-sdk": "^2.1388.0",
    "axios": "^0.27.2",
    "axios-cache-adapter": "^2.7.3",
    "bignumber.js": "^9.0.1",
    "bull": "^4.10.4",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.0.0",
    "csv-file-validator": "^2.0.0",
    "deep-parse-json": "^1.0.2",
    "ethers": "^5.7.1",
    "express-session": "^1.17.3",
    "file-type": "^16.5.3",
    "generate-password": "^1.7.0",
    "handlebars": "^4.7.7",
    "keyv": "4.5.0",
    "lodash": "^4.17.20",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^4.0.3",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.7.2",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "randomstring": "^1.2.2",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "sharp": "^0.30.7",
    "socket.io-client": "^4.4.1",
    "swagger-ui-express": "^4.1.6",
    "telegraf": "4.10.0",
